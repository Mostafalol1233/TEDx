{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/ws", "destination": "/api" }
  ],
  "headers": [
    {
      "source": "/ws",
      "headers": [
        { "key": "Upgrade", "value": "$http_upgrade" },
        { "key": "Connection", "value": "upgrade" }
      ]
    }
  ]
}